{
  "codeLens.enable": true,
  "emmet.includeLanguages": {
    "vue": "html",
    "javascript": "javascriptreact",
    "js": "javascriptreact"
  },
  "list.source.files.excludePatterns": [
    "**/node_modules/**",
    "**/node_modules/**/.*",
    "**/node_modules/**/.*/**",
    "**/.git/**"
  ],
  "coc.preferences.formatOnType": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "json",
    "javascriptreact",
    "typescript.tsx",
    "graphql",
    "html",
    "vue"
  ],
  "snippets.ultisnips.directories": ["UltiSnips"],
  "snippets.extends": {
    "javascriptreact": ["javascript"],
    "typescript": ["javascript"]
  },
  "cSpell.language": "en-GB",
  "cSpell.userWords": [
    "Denon",
    "Slicemaster",
    "Slicemasters",
    "browserslist",
    "deno",
    "destructure",
    "devtools",
    "githubusername",
    "noopener",
    "noreferrer",
    "placeholder",
    "preact",
    "qraphql",
    "repos",
    "snowpack",
    "subgrid",
    "ultisnips",
    "unselectable",
    "whitesmoke",
    "youtube"
  ],
  "signature.preferShownAbove": false,
  "signature.target": "echo",
  "explorer.icon.enableNerdfont": true,
  "explorer.previewAction.onHover": false,
  "explorer.keyMappings.global": {
    "v": "open:vsplit",
    "<cr>": ["expandable?", "expand", "open"]
  }
}
