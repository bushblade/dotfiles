{
  "emmet.includeLanguages": {
    "vue": "html",
    "javascript": "javascriptreact",
    "js": "javascriptreact"
  },
  "list.source.files.excludePatterns": [
    "**/node_modules/**",
    "**/node_modules/**/.*",
    "**/node_modules/**/.*/**",
    "**/.git/**"
  ],
  "coc.preferences.formatOnType": true,
  "snippets.ultisnips.directories": ["UltiSnips"],
  "snippets.extends": {
    "javascriptreact": ["javascript"]
  }
}
